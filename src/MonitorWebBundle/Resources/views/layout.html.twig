{% set title = 'Drom Monitor 3.0' %}
<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Drom Monitor 3.0</title>
    <link rel="shortcut icon" href="{{ absolute_url(asset('favicon.png')) }}">
    <link rel="apple-touch-icon" href="{{ absolute_url(asset('favicon.png')) }}">
    {% block stylesheet %}
        <!-- Bootstrap core CSS -->
        <link href="{{ asset('bundles/monitorweb/assets/css/bootstrap.min.css') }}" rel="stylesheet">
        <!-- Custom styles for this template -->
        <link href="{{ asset('bundles/monitorweb/assets/css/test.css') }}" rel="stylesheet">
    {% endblock %}
    {% block head_js %}
        <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
        <script src="https://code.angularjs.org/1.5.0-rc.0/angular.min.js"></script>
        <script src="https://code.angularjs.org/1.5.0-rc.0/angular-route.min.js"></script>
        <script src="https://code.angularjs.org/1.5.0-rc.0/angular-resource.min.js"></script>
    {% endblock %}
</head>
<body ng-app>
{% block body %}
    {% include '@MonitorWeb/_common/header.html.twig' %}
    <div class="container-fluid">
        <div class="row">
            {% include '@MonitorWeb/_common/left.html.twig' %}
            <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
                {% block content %}
                {% endblock %}
                {% block fos_user_content %}
                {% endblock fos_user_content %}
            </div>
        </div>
    </div>
    {% block javascripts %}
        {% include 'MonitorWebBundle:_common:scripts.html.twig' %}
    {% endblock %}
{% endblock %}
</body>
</html>
