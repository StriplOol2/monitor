{% set title = 'Drom Monitor 3.0' %}<!DOCTYPE html>
<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" href="{{ absolute_url(asset('favicon.png')) }}">
    <link rel="apple-touch-icon" href="{{ absolute_url(asset('favicon.png')) }}">

    <title>{{ title }}</title>

    {% block stylesheet %}
        <!-- Bootstrap core CSS -->
        <link href="{{ asset('bundles/monitorweb/assets/css/bootstrap.min.css') }}" rel="stylesheet">
        <!-- Custom styles for this template -->
        <link href="{{ asset('bundles/monitorweb/assets/css/sign.bootstrap.css') }}" rel="stylesheet">
    {% endblock %}

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>
{% block content %}
{% endblock %}
</body>
</html>
